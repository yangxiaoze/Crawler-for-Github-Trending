<div class="container-fluid" style="font-size: 90%">
  
  <div class="row">
    {{> menu }}
  </div>
  
  <div class="row"> </div>

  <h2 class="heading">
      Services of LobbyCentral, total {{ listOfServices.length }} records in {{ pages }} pages. 
  </h2>

  <div class="row">
    <div class="table-responsive">
      <table class="table">
        <thead>
          <tr>
            <th>Service Name</th>
            <th>Kiosk Name</th>
            <th>Is Kiosk Default</th>
            <th>Category</th>
            <th>Status</th>
          </tr>
        </thead>
        <tbody>
          {{#each listOfServices}}
          <tr class="">
            <td><strong>
              {{ this.serviceName}}
            </strong></td>
            <td>
              {{ this.kioskName}}
            </td>
            <td>
              {{ this.isKioskDefault}}
            </td>
            <td>
              {{ this.category}}
            </td>
            <td>
              {{ this.status}}
            </td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>