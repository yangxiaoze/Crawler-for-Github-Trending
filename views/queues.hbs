<div class="container-fluid col-xs-12" style="font-size: 90%">

  <div class="row">
    {{> resources }}
  </div>
  
  <div class="row">  </div>

  <h2 class="heading">
    <center>
      Queue of LobbyCentral, Total {{listOfQueues.length}}. 
    </center>
  </h2>
 
  <div class="row">
    <div class="table-responsive">
      <table class="table">
        <thead>
          <tr>
            <th>Name</th>
            <th>Type</th>
            <th>Location</th>
            <th>Status</th>
          </tr>
        </thead>
        <tbody>
          {{#each listOfQueues}}
          <tr class="">
            <td><strong>
              {{ this.queueName}}
            </strong></td>
            <td>
              {{ this.queueType}}
            </td>
            <td>
              {{ this.location}}
            </td>
            <td>
              {{ this.status}}
            </td>
          </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>